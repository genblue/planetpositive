          "_name": "features11",
          "_customHTML": "<section class=\"features11 video-with-lightbox\" group=\"Tabs & Accordions\" plugins=\"amp-main, font-awesome, amp-lightbox, amp-youtube, amp-accordion\">\n    <!--  -->\n    \n   \n    \n    \n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <!--  -->\n        <header>Title:</header>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text:\" name=\"showText\" checked>\n        <select title=\"Align:\" name=\"alignTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Button:\" name=\"showButton\" checked>\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#f7f7f7\" selected>\n            <input type=\"image\" title=\"Background Image:\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Content -->\n        <div class=\"mbr-row\">\n            <!-- Left-side -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6\">\n                 <p style=\"margin:0px 0px 10px 0px;text-align:center;font-size:17px;line-height:150%;color:#000;font-weight:bold;\">UP NEXT</p>\n                 <iframe class=\"airtable-embed\" src=\"https://airtable.com/embed/shroX0FxSmeSTDLNI?backgroundColor=pink\" frameborder=\"0\" onmousewheel width=\"100%\" height=\"533\" style=\"background: transparent; border: 1px solid #ccc;\"></iframe>\n                  <!-- \n                <div class=\"img\">\n                        \n                \n                      <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/hosts-2-468x432.png\" alt=\"image\" layout=\"responsive\" width=\"238.33333333333331\" height=\"220\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/hosts-2-468x432.png\" alt title> \n                        \n                    </amp-img>\n\n\n                </div>\n            -->\n            </div>\n            <!-- Right-side -->\n            <div class=\"mbr-col-md-12 mbr-col-sm-12 mbr-col-lg-6\">\n                <!-- Content-block -->\n                <div class=\"b-content mbr-py-5\">\n                    <!-- Title -->\n                    <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">Weekly Wisdom &amp; Opportunities</h3>\n                    \n                    <!-- Subtitle -->\n                    <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-pt-3 mbr-light\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\"><span style=\"font-style: normal;\"><u>Every TUESDAY @ 4:00 PM UTC</u></span><div><span style=\"font-style: normal;\">9:00 AM, Los Angeles</span></div><span style=\"font-style: normal;\">12:00 PM, New York</span><br><div><span style=\"font-style: normal;\">1:00 PM, Rio de Janeiro<br>5:00 PM, London</span><br><span style=\"font-style: normal;\">7:00 PM, Moscow</span></div><div><span style=\"font-style: normal;\">9:30 PM, Mumbai<br>12:00 AM (Wed), Singapore<br>2:00 AM (Wed), Sydney</span><br></div></h4>\n                    <!-- Text -->\n                    <p id=\"summary\" class=\"mbr-text mbr-fonts-style mbr-pt-2 mbr-light\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">Please join us for an exclusive look at the people and technologies addressing our greatest challenges and enabling humans to thrive in harmony with Earth.<br></p>\n                    <!-- Button -->\n                    \n                    <div mbr-buttons class=\"mbr-section-btn mbr-pt-5 btn-custom\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButton\"><a class=\"btn-danger btn\" href=\"https://us02web.zoom.us/meeting/register/tJIkcOCupz8qgVpqWd7N_OhE3LQdtx5A1A\" data-app-placeholder=\"Type Text\" target=\"_blank\">REGISTER / JOIN</a>\n                        \n                        \n                    <p style=\"margin:40px 0px 10px 0px;text-align:center;font-size:17px;line-height:150%;color:#000;font-weight:bold;\">Add event to calendar</p><p style=\"margin:0px 0px 10px 0px;text-align:center;font-size:0px;\"><a href=\"https://www.addevent.com/event/kM5090308+apple\" title=\"Apple\" target=\"_blank\" style=\"display:inline;\" data-app-placeholder=\"Type Text\" class><img src=\"https://w